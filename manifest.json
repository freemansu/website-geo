{
    "manifest_version": 2,
    "name": "website-geo",
    "description": "This extension display geographic location of website",
    "version": "1.0",
    "browser_action": {
        "default_icon": "icon.png",
        "default_popup": "popup.html"
    },
    "permissions": [
        "activeTab",
        "https://ajax.googleapis.com/"
    ]
}